using System;

public class User
{
    public User(string firstName, string lastName, string username, string password)
    {
        FirstName = firstName;
        LastName = lastName;
        Username = username;
        Password = password;
    }
}

class Program
{
    static void Main(string[] args)
    {
        // Creating a new user
        User newUser = new User("John", "Doe", "johndoe", "password123");
    }
}
